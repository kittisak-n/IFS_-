                     <a-select-option\r\n                        v-for=\"item in dataProvinces\"\r\n                        :key=\"item.key\"\r\n                        :value=\"item.value\"\r\n                      >\r\n                        {{ item.name_th }}\r\n                      </a-select-option>\r\n                    </a-select>\r\n                  </a-col>\r\n                  <a-col :span=\"2\" style=\"text-align:end\">\r\n                    <b> อำเภอ:</b>\r\n                  </a-col>\r\n                  <a-col :span=\"4\">\r\n                    <a-select\r\n                      show-search\r\n                      placeholder=\"โปรดเลือกอำเภอ\"\r\n                      option-filter-prop=\"children\"\r\n                      style=\"width: 100%\"\r\n                      v-model=\"person.amphure_id\"\r\n                      :filter-option=\"filterOptionAmphure\"\r\n                      @change=\"getAllDistrict()\"\r\n                    >\r\n                      <a-select-option\r\n                        v-for=\"item in dataAmphures\"\r\n                        :key=\"item.key\"\r\n                        :value=\"item.value\"\r\n                      >\r\n                        {{ item.name_th }}\r\n                      </a-select-option>\r\n                    </a-select>\r\n                  </a-col>\r\n                  <a-col :span=\"2\" style=\"text-align:end\">\r\n                    <b> ตำบล:</b>\r\n                  </a-col>\r\n                  <a-col :span=\"4\">\r\n                    <a-select\r\n                      show-search\r\n                      placeholder=\"โปรดเลือกตำบล\"\r\n                      option-filter-prop=\"children\"\r\n                      style=\"width: 100%\"\r\n                      v-model=\"person.district_id\"\r\n                      :filter-option=\"filterOptionDistrict\"\r\n                      @change=\"getAllDistrict()\"\r\n                    >\r\n                      <a-select-option\r\n                        v-for=\"item in dataDistricts\"\r\n                        :key=\"item.key\"\r\n                        :value=\"item.value\"\r\n                      >\r\n                        {{ item.name_th }}\r\n                      </a-select-option>\r\n                    </a-select>\r\n                  </a-col>\r\n                  <a-col :span=\"2\" style=\"text-align:end\">\r\n                    <b>รหัสไปรษณีย์:</b>\r\n                  </a-col>\r\n                  <a-col :span=\"4\">\r\n                    <a-select\r\n                      style=\"width:100%\"\r\n                      v-model=\"person.district_id\"\r\n                      disabled\r\n                    >\r\n                      <a-select-option :value=\"null\">\r\n                        โปรดเลือก\r\n                      </a-select-option>\r\n                      <a-select-option\r\n                        v-for=\"item in dataDistricts\"\r\n                        :key=\"item.key\"\r\n                        :value=\"item.value\"\r\n                      >\r\n                        {{ item.zip_code }}\r\n                      </a-select-option>\r\n                    </a-select>\r\n                  </a-col>\r\n                </a-row>\r\n              </div>\r\n              <br />\r\n            </a-col>\r\n          </a-row>\r\n          <a-row :gutter=\"[8, 8]\">\r\n            <a-col\r\n              class=\"form-addUser\"\r\n              v-if=\"current == 1\"\r\n              :span=\"24\"\r\n              style=\"margin: 0.2em 0px;\"\r\n            >\r\n              <br />\r\n              <a-row\r\n                :gutter=\"[10, 10]\"\r\n                v-for=\"(item, index) in person.position_access\"\r\n                :key=\"item.key\"\r\n                type=\"flex\"\r\n                justify=\"center\"\r\n              >\r\n                <a-col :span=\"10\">\r\n                  <a-select\r\n                    show-search\r\n                    placeh