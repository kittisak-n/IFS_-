ce=\"course_data.sub_data\"\r\n                    :pagination=\"false\"\r\n                  >\r\n                    <span slot=\"section_number\" slot-scope=\"text\">\r\n                      <div\r\n                        :style=\"{\r\n                          textAlign: 'center',\r\n                          margin: 'auto',\r\n                        }\"\r\n                      >\r\n                        {{ text }}\r\n                      </div>\r\n                    </span>\r\n                    <span\r\n                      slot=\"section_student\"\r\n                      slot-scope=\"text, records, sub_index\"\r\n                    >\r\n                      <div\r\n                        :style=\"{\r\n                          textAlign: 'center',\r\n                          margin: 'auto',\r\n                        }\"\r\n                      >\r\n                        <a-input\r\n                          style=\"width:100%\"\r\n                          v-model=\"\r\n                            course_data[index].sub_data[sub_index]\r\n                              .section_student\r\n                          \"\r\n                        />\r\n                      </div>\r\n                    </span>\r\n                    <span\r\n                      slot=\"count_group_lecture\"\r\n                      slot-scope=\"text, records, sub_index\"\r\n                    >\r\n                      <div\r\n                        :style=\"{\r\n                          textAlign: 'center',\r\n                          margin: 'auto',\r\n                        }\"\r\n                      >\r\n                        <a-input\r\n                          style=\"width:100%\"\r\n                          v-model=\"\r\n                            course_data[index].sub_data[sub_index]\r\n                              .count_group_lecture\r\n                          \"\r\n                        />\r\n                      </div>\r\n      